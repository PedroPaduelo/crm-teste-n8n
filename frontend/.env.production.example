# Configurações de Produção do Frontend

# URL da API do Backend (Produção)
VITE_API_URL=https://api.seu-dominio.com

# Configurações do N8N (Produção)
VITE_N8N_WEBHOOK_URL=https://n8n.seu-dominio.com/webhook

# Configurações do WhatsApp (se necessário acesso direto)
VITE_WHATSAPP_PHONE_NUMBER_ID=seu-id-numero-whatsapp-business

# Configurações de Ambiente
VITE_NODE_ENV=production

# Configurações de Analytics (se necessário)
VITE_GA_TRACKING_ID=seu-google-analytics-tracking-id

# Configurações de Mapas (se necessário)
VITE_MAPBOX_API_KEY=sua-chave-api-do-mapbox

# Configurações de Pagamento (se necessário)
VITE_STRIPE_PUBLISHABLE_KEY=sua-chave-publicavel-stripe

# Configurações de Monitoramento (se necessário)
VITE_SENTRY_DSN=seu-dsn-do-sentry-para-monitoramento-frontend

# Configurações de WebSocket (se necessário)
VITE_WS_URL=wss://api.seu-dominio.com

# Configurações de CDN (se necessário)
VITE_CDN_URL=https://cdn.seu-dominio.com

# Configurações de Recaptcha (se necessário)
VITE_RECAPTCHA_SITE_KEY=sua-chave-site-recaptcha

# Configurações de Features Flags (se necessário)
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_CHAT_SUPPORT=false
VITE_ENABLE_NOTIFICATIONS=true

# Configurações de API Externas (se necessário)
VITE_EXTERNAL_API_KEY=sua-chave-api-externa
VITE_EXTERNAL_API_URL=https://api.externa.com

# Configurações de Desenvolvimento (deixe false em produção)
VITE_ENABLE_DEBUG=false
VITE_ENABLE_CONSOLE_LOGS=false